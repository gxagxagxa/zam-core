        "regex": ":([\\w\\d\\-\\{\\}\\*\\.]+)",
        "component_mappings": [
          { "name": "pipeline_step", "regex": null, "component_mappings": null }
        ]
      },
      {
        "name": "artist",
        "regex": ":([\\w\\d\\-\\{\\}\\*\\.]+)",
